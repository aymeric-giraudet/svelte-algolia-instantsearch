{
  "name": "svelte-algolia-instantsearch",
  "version": "0.8.0",
  "license": "MIT",
  "description": "Svelte wrapper for Algolia InstantSearch",
  "repository": "aymeric-giraudet/svelte-algolia-instantsearch",
  "keywords": [
    "svelte",
    "algolia",
    "instantsearch",
    "search",
    "instantsearch.js"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "release": "shipjs prepare"
  },
  "dependencies": {
    "algoliasearch-helper": "^3.11.2",
    "instantsearch.js": "^4.49.4"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^2.0.2",
    "@sveltejs/kit": "^1.5.3",
    "@sveltejs/package": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "algoliasearch": "^4.14.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "shipjs": "0.25.1",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.25.3"
  },
  "peerDependencies": {
    "algoliasearch": ">= 3.1 < 5",
    "svelte": ">= 3"
  },
  "type": "module"
}
